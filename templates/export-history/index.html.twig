{% endblock %}

{% block body %}
    <div class="container-fluid ">
        <div class="row">
            <div class="col col-lg-2">
                {% include 'export-history/form-filter.html.twig' %}
            </div>

            <div class="col">
                {% include 'export-history/table.html.twig' %}
            </div>
        </div>
    </div>
{% endblock %}
